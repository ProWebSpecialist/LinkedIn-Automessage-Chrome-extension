{
  "manifest_version": 2,
  "name": "LinkedIn AutoMessage",
  "description": "Automatically clicks all 'Connect' buttons available on LinkedIn 'Search People' pages.",
  "version": "1.0.17",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons" : {
  	"16": "icon.png",
  	"128": "icon_128x128.png"
  },

  "permissions": ["activeTab", "tabs", "*://*.linkedin.com/*", "storage","browsingData"]
}
